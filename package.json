    "prisma:generate": "prisma generate",
    "vercel:build": "npm prisma:generate"
  },
  "prisma": {
    "seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.1",
