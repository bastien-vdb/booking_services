    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "date-fns": "^2.30.0",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.8",
    "next": "13.4.8",
    "next-auth": "^4.22.1",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-big-calendar": "^1.8.1",
    "react-calendar": "^4.3.0",
    "react-day-picker": "^8.8.0",
    "react-dom": "18.2.0",
    "stripe": "^12.13.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  },
  "devDependencies": {
