{
  "name": "rdvbook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm run vercel:build && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:migrate": "prisma migrate dev",
    "prisma:setup": "prisma db push && npm prisma:seed && npm prisma:generate",
